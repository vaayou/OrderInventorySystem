MYSQL_ROOT_PASSWORD=root
MYSQL_DATABASE=db
MYSQL_USER=app_user
MYSQL_PASSWORD=root

# JDBC url used by Spring Boot (uses mysql service hostname from docker-compose)
SPRING_DATASOURCE_URL=jdbc:mysql://mysql:3306/db?useSSL=false&allowPublicKeyRetrieval=true&serverTimezone=UTC

# API URL exposed to frontend (used in build)
# For nginx proxy setup set this to empty so frontend uses relative URLs
REACT_APP_API_URL=

# Ports
BACKEND_PORT=8080
FRONTEND_PORT=80
